{
  "name": "bot",
  "author": "Vedinsoh",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "build": "tsc",
    "start": "TS_NODE_BASEURL=dist node -r tsconfig-paths/register ./dist/index.js",
    "start:dev": "tsc-watch --noClear --onSuccess \"npm run start\"",
    "compose": "docker compose up --build -d",
    "compose:dev": "./scripts/compose-dev.sh",
    "lint": "eslint src --ext .ts",
    "prettier": "prettier --write src"
  },
  "dependencies": {
    "bufferutil": "^4.0.7",
    "discord-hybrid-sharding": "^2.1.2",
    "discord.js": "^14.11.0",
    "dotenv": "^16.0.3",
    "glob": "^10.2.5",
    "mongoose": "^7.2.0",
    "p-queue": "^7.3.4",
    "pino": "^8.14.1",
    "pino-pretty": "^10.0.0",
    "re2": "^1.18.0",
    "redis": "^4.6.6",
    "tsconfig-paths": "^4.2.0",
    "url-regex-safe": "^3.0.0",
    "utf-8-validate": "^6.0.3",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@sapphire/ts-config": "^4.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/glob": "^8.1.0",
    "@types/node": "^18.16.13",
    "@types/url-regex-safe": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.0.4"
  },
  "imports": {
    "#config": "./dist/config.js",
    "#client": "./dist/AutoPublisher.js",
    "#constants/*": "./dist/constants/*.js",
    "#crosspost/*": "./dist/crosspost/*.js",
    "#data": "./dist/data/index.js",
    "#managers/*": "./dist/managers/*.js",
    "#services": "./dist/services/index.js",
    "#schemas/*": "./dist/schemas/*.js",
    "#structures/*": "./dist/structures/*.js",
    "#types/*": "./dist/types/*.js",
    "#util/*": "./dist/util/*.js"
  },
  "engines": {
    "node": "^20"
  },
  "type": "module"
}
