{
  "name": "discord-auto-publisher",
  "version": "2.0.0",
  "description": "Automatically publish messages or news in your announcement channels!",
  "author": "Forcellrus",
  "license": "GPL-3.0-or-later",
  "homepage": "https://github.com/Forcellrus/Discord-Auto-Publisher#readme",
  "repository": "github:Forcellrus/Discord-Auto-Publisher",
  "keywords": [
    "discord",
    "bot",
    "discord.js",
    "publisher"
  ],
  "scripts": {
    "start": "rm -rf dist && yarn run build && yarn run start:prod",
    "start:dev": "ts-node-dev -r tsconfig-paths/register src/index.ts",
    "start:prod": "node -r tsconfig-paths/register dist/index.js",
    "build": "tsc",
    "build:watch": "tsc -w",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "bufferutil": "^4.0.3",
    "discord-hybrid-sharding": "^1.4.3",
    "discord.js-light": "^4.3.1",
    "dotenv": "^14.2.0",
    "glob": "^7.2.0",
    "p-queue": "^6.6.2",
    "pino": "^7.6.4",
    "pino-pretty": "^7.5.0",
    "re2": "^1.17.3",
    "url-regex-safe": "^3.0.0",
    "utf-8-validate": "^5.0.4"
  },
  "devDependencies": {
    "@sapphire/ts-config": "^3.1.8",
    "@types/glob": "^7.2.0",
    "@types/node": "^17.0.10",
    "@types/url-regex-safe": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.2"
  },
  "imports": {
    "#config": "./dist/config.json",
    "#cluster": "./dist/index.js",
    "#client": "./dist/AutoPublisher.js",
    "#modules/*": "./dist/modules/*.js",
    "#functions/*": "./dist/functions/*.js",
    "#structures/*": "./dist/structures/*.js",
    "#types/*": "./dist/types/*.js",
    "#util/*": "./dist/util/*.js",
    "#blacklist": "./dist/serversBlacklist.json"
  },
  "engines": {
    "node": "^16.6.0",
    "npm": "^7.0.0",
    "yarn": "^1.0.0"
  }
}
