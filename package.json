{
  "name": "discord-auto-publisher",
  "version": "2.0.1",
  "description": "Automatically publish messages or news in your announcement channels!",
  "author": "Forcellrus",
  "license": "GPL-3.0-or-later",
  "homepage": "https://github.com/Forcellrus/Discord-Auto-Publisher#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Forcellrus/Discord-Auto-Publisher.git"
  },
  "bugs": {
    "url": "https://github.com/Forcellrus/Discord-Auto-Publisher/issues"
  },
  "keywords": [
    "discord",
    "bot",
    "discord.js",
    "publisher"
  ],
  "scripts": {
    "start": "yarn run build && yarn run start:prod",
    "start:dev": "nodemon --watch 'src/**/*.ts' --exec ts-node src/index.ts",
    "start:prod": "node -r tsconfig-paths/register dist/index.js",
    "build": "rm -rf dist && tsc",
    "build:watch": "rm -rf dist && tsc -w",
    "reinstall": "rm -rf node_modules && yarn install",
    "lint": "eslint src --ext .ts"
  },
  "dependencies": {
    "@joshdb/core": "^1.2.7",
    "@joshdb/json": "^1.0.5",
    "axios": "^0.26.0",
    "bufferutil": "^4.0.3",
    "discord-api-types": "^0.27.1",
    "discord-hybrid-sharding": "^1.4.4",
    "discord.js-light": "^4.6.0",
    "dotenv": "^16.0.0",
    "glob": "^7.2.0",
    "p-queue": "^6.6.2",
    "pino": "^7.8.0",
    "pino-pretty": "^7.5.1",
    "re2": "^1.17.3",
    "url-regex-safe": "^3.0.0",
    "utf-8-validate": "^5.0.4"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/node": "^17.0.18",
    "@types/url-regex-safe": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.2"
  },
  "imports": {
    "#config": "./dist/config.json",
    "#client": "./dist/AutoPublisher.js",
    "#modules/*": "./dist/modules/*.js",
    "#functions/*": "./dist/functions/*.js",
    "#structures/*": "./dist/structures/*.js",
    "#types/*": "./dist/types/*.js",
    "#util/*": "./dist/util/*.js"
  },
  "engines": {
    "node": "^16.6.0",
    "npm": "^7.0.0",
    "yarn": "^1.0.0"
  }
}
