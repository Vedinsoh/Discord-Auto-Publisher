{
  "name": "discord-auto-publisher",
  "version": "3.0.0",
  "description": "Automatically publish messages or news in your announcement channels!",
  "author": "Forcellrus",
  "license": "GPL-3.0-or-later",
  "homepage": "https://github.com/Forcellrus/Discord-Auto-Publisher#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Forcellrus/Discord-Auto-Publisher.git"
  },
  "bugs": {
    "url": "https://github.com/Forcellrus/Discord-Auto-Publisher/issues"
  },
  "keywords": [
    "discord",
    "bot",
    "discord.js",
    "publisher"
  ],
  "scripts": {
    "build": "tsc",
    "start": "cross-env TS_NODE_BASEURL=dist node -r tsconfig-paths/register dist/index.js",
    "start:dev": "tsc-watch --noClear --onSuccess \"yarn run start\"",
    "compose": "docker compose up --build -d",
    "compose:dev": "docker compose -f docker-compose.yml -f docker-compose.development.yml up --build",
    "reinstall": "rm -rf node_modules && yarn install",
    "update": "yarn upgrade-interactive",
    "lint": "eslint src --ext .ts",
    "prettier": "prettier --write src"
  },
  "dependencies": {
    "bufferutil": "^4.0.7",
    "cross-env": "^7.0.3",
    "discord-hybrid-sharding": "^1.7.5",
    "discord.js-light": "^4.10.0",
    "dotenv": "^16.0.3",
    "glob": "^8.0.3",
    "p-queue": "^7.3.0",
    "pino": "^8.7.0",
    "pino-pretty": "^9.1.1",
    "re2": "^1.18.0",
    "redis": "^4.5.1",
    "tsconfig-paths": "^4.1.1",
    "url-regex-safe": "^3.0.0",
    "utf-8-validate": "^5.0.10",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@sapphire/ts-config": "^3.3.4",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/glob": "^8.0.0",
    "@types/node": "^18.11.13",
    "@types/url-regex-safe": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.1",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.4"
  },
  "imports": {
    "#config": "./dist/config.js",
    "#client": "./dist/AutoPublisher.js",
    "#constants/*": "./dist/constants/*.js",
    "#crosspost/*": "./dist/crosspost/*.js",
    "#functions/*": "./dist/functions/*.js",
    "#handlers": "./dist/handlers/*.js",
    "#managers/*": "./dist/managers/*.js",
    "#schemas/*": "./dist/schemas/*.js",
    "#structures/*": "./dist/structures/*.js",
    "#types/*": "./dist/types/*.js",
    "#util/*": "./dist/util/*.js"
  },
  "engines": {
    "node": "^18.12.0",
    "npm": "please-use-yarn"
  },
  "type": "module",
  "packageManager": "yarn@3.2.4"
}
