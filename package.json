{
  "name": "discord-auto-publisher",
  "version": "3.0.0",
  "description": "Automatically publish messages or news in your announcement channels!",
  "author": "Forcellrus",
  "license": "GPL-3.0-or-later",
  "homepage": "https://github.com/Forcellrus/Discord-Auto-Publisher#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Forcellrus/Discord-Auto-Publisher.git"
  },
  "bugs": {
    "url": "https://github.com/Forcellrus/Discord-Auto-Publisher/issues"
  },
  "keywords": [
    "discord",
    "bot",
    "discord.js",
    "publisher"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc",
    "start": "cross-env TS_NODE_BASEURL=dist NODE_ENV=production node -r tsconfig-paths/register dist/index.js",
    "start:dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register src/index.ts",
    "reinstall": "rm -rf node_modules && yarn install",
    "update": "yarn upgrade-interactive --latest",
    "lint": "eslint src --ext .ts",
    "prettier": "prettier --write src"
  },
  "dependencies": {
    "bufferutil": "^4.0.3",
    "cross-env": "^7.0.3",
    "discord-hybrid-sharding": "^1.7.1",
    "discord.js-light": "^4.9.0",
    "dotenv": "^16.0.1",
    "glob": "^8.0.3",
    "pino": "^8.4.0",
    "pino-pretty": "^8.1.0",
    "re2": "^1.17.7",
    "redis": "^4.2.0",
    "tsconfig-paths": "^4.1.0",
    "url-regex-safe": "^3.0.0",
    "utf-8-validate": "^5.0.9"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/glob": "^7.2.0",
    "@types/node": "^18.6.4",
    "@types/url-regex-safe": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "imports": {
    "#config": "./dist/config.js",
    "#client": "./dist/AutoPublisher.js",
    "#constants/*": "./dist/constants/*.js",
    "#functions/*": "./dist/functions/*.js",
    "#handlers": "./dist/handlers/*.js",
    "#managers/*": "./dist/managers/*.js",
    "#structures/*": "./dist/structures/*.js",
    "#types/*": "./dist/types/*.js",
    "#util/*": "./dist/util/*.js"
  },
  "engines": {
    "node": ">=16.9.0",
    "yarn": ">=1.12.0",
    "npm": "please-use-yarn"
  }
}
